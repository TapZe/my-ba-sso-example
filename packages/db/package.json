{
  "name": "@my-sso/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:start": "docker compose --env-file ../../apps/server/.env up -d",
    "db:watch": "docker compose  --env-file ../../apps/server/.env up",
    "db:stop": "docker compose stop",
    "db:down": "docker compose down"
  },
  "devDependencies": {
    "prisma": "^6.15.0",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:"
  }
}
